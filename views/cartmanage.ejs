  <script>
    const qtyChange = (proId, count) => {

      const quantity = document.getElementById(proId + 'minus').value

      $.ajax({
        url: `/qtyChange?id=${proId}&expressionKey=${count}&currentQuantity=${quantity}`,
        method: 'POST',
        success: (response) => {
          
          if (response.remove == true) {

            document.getElementById(proId).remove()
            Swal.fire(
              'Deleted!',
              'Your file has been deleted.',
              'success'
            )
          } else {

            document.getElementById(proId+'minus').value = response.newQuantityCount
            document.getElementById(proId+'result').value = response.newQuantityCount
          }
          document.getElementById('totalPrice2').innerHTML = `₹ ${response.totalPrice}`
          document.getElementById('totalPrice3').innerHTML = `₹ ${response.totalPrice}`

        }
      })
    }







    const removeItem = (productId) => {


      const id = productId


      Swal.fire({
        title: 'Delete this cart item?',

        icon: 'warning',
        showCancelButton: true,
        confirmButtonColor: '#3085d6',
        cancelButtonColor: '#d33',
        confirmButtonText: 'Yes, delete it!'
      }).then((result) => {
        if (result.isConfirmed) {
          $.ajax({
            url: `/removeCartItem?id=${id}`,
            method: "POST",
            success: (response) => {
              if (response.remove == true) {
                document.getElementById(productId).remove()


                document.getElementById('totalPrice2').innerHTML = `₹ ${response.totalPrice}`
                document.getElementById('totalPrice3').innerHTML = `₹ ${response.totalPrice}`

              }


            }
          })

          Swal.fire(
            'Deleted!',
            'Your file has been deleted.',
            'success'
          )
        }
      })

    }


    function deleteAlert(id) {
      Swal.fire({
        title: ' <h4>Delete Product? </h4>',
        icon: 'warning',

        showCloseButton: true,
        showCancelButton: true,
        focusConfirm: false,
        confirmButtonText:
          '<a class="text-light " href="/admin/deletecartitem?id=' + id + '">Delete</a>',

        cancelButtonText:
          'Cancel',


      })
    }
