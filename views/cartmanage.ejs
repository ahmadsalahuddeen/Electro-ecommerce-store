                    <button onclick="qtyChange('<%= items.product._id %>',-1)"
                      class="quantity__minus"><span>-</span></button>


                    <input readonly name="qty" id="<%= items.product._id%>result" type="text" class="quantity__input" value="<%= items.qty %>">
