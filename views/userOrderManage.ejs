                        <div class="d-sm-flex text-center text-sm-start"><a class="d-inline-block flex-shrink-0 mx-auto"
                            href="/productdetail?id=<%= order.product._id%>" style="width: 10rem;"><img class="rounded"
                              src="<%= order.product.image[0] %>" alt="Product Image"></a>
                          <div class="ps-sm-4 pt-2">
                            <h3 class="product-title fs-base mb-2"><a href="/productdetail?id=<%= order.product._id%>">
                                <%= order.product.name %>
                              </a></h3>
                            <div class="fs-sm"><span class="text-muted me-2">Category:</span>
                              <%= order.product.category.name %>
                            </div>

                            <div class="fs-lg text-accent pt-2">₹ <%= order.product.discount %>
                            </div>
                          </div>
                        </div>
                        <div class="pt-2 ps-sm-3 mx-auto mx-sm-0 text-center">
                          <div class="text-muted mb-2">Quantity:</div>
                          <%= order.qty %>
                        </div>
                        <div class="pt-2 ps-sm-3 mx-auto mx-sm-0 text-center">
                          <div class="text-muted mb-2">Subtotal</div>₹ <%= order.product.discount * order.qty %>
                        </div>
                      </div>
                      <!-- Item-->
                      <% }); %>

                        <!-- Footer-->
                        <div class="modal-footer flex-wrap justify-content-between bg-secondary fs-md">

                          <div class="px-2 py-1"><span class="text-muted">Total:&nbsp;</span><span class="fs-xl"> ₹ <%=
                                element.totalPrice %> </span></div>
                        </div>
                  </div>
                </div>
              </div>
            </div>

            <%}); %>
