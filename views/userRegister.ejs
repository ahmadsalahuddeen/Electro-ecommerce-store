<h2 class="h4 mb-3">No account? create one!</h2>
<p class="fs-sm text-muted mb-4">Already have an account? Then <a href="/login">click here</a> to login.
</p>
<form method="post" action="/register" enctype="multipart/form-data">
  <div class="row gx-4 gy-3">

    <div class="col-sm-6">
      <label class="form-label" for="reg-ln">Name</label>
      <input class="form-control" type="text" required name="name">
      <div class="invalid-feedback">Please enter your name!</div>
    </div>
    <div class="col-sm-6">
      <label class="form-label" for="reg-email">E-mail Address</label>
      <input class="form-control" type="email" required name="email" >
      <div class="invalid-feedback">Please enter valid email address!</div>
    </div>
    <div class="col-sm-6">
      <label class="form-label" for="reg-phone">Phone Number</label>
      <input class="form-control" type="number" name="mobile" required>
      <div class="invalid-feedback">Please enter your phone number!</div>
    </div>
    
    <div class="col-sm-6">
      <label class="form-label" for="reg-password">Password</label>
      <input class="form-control" minlength="8" type="password" required name="password">
      <div class="invalid-feedback">Please enter password!</div>
    </div>
    <div class="col-sm-6">
      <label class="form-label" for="reg-password">Confirm Password</label>
      <input class="form-control" minlength="8" type="password" required name="confirmpassword">

      <div class="invalid-feedback">password is not the same!</div>
    </div>

    <div class="col-12 text-end">
      <button class="btn btn-primary" type="submit"><i class="ci-user me-2 ms-n1"></i>Sign Up</button>
    </div>
  </div>
</form>
<% if(typeof message !== 'undefined') { %>
  <div class="message-login"><%= message %></div>

  <% } %>
